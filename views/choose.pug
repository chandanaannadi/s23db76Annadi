extends layout

block content
    h1= Choose

    - var image_names=["Chicken.jpg","cheese.jpg","veg.jpg","grilled_chicken.jpg","BurstPizza.jpg"]
    - for(let i=0;i<3;i++){
        - let randomNumber=Math.floor(Math.random()*5)
        img(src=`/images/${image_names[randomNumber]}` alt=`${randomNumber}` height="300px" width="300px") 
    - }